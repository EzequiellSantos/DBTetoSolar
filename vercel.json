{
  "version": 2,
  "name": "frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "frontend/(.*).js",
      "headers": { "content-type": "application/javascript" },
      "dest": "/$1.js"
    },
    {
      "src": "frontend/(.*).js",
      "headers": { "content-type": "text/css" },
      "dest": "frotend/$1.css"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}

